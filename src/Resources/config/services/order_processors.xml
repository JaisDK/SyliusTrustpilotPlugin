<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="false" />

        <service id="setono_sylius_trustpilot.orders_processor"
                 class="Setono\SyliusTrustpilotPlugin\Trustpilot\Order\Processor\TrustpilotOrdersProcessor">
            <argument type="service" id="setono_sylius_trustpilot.order_provider.latest" />
            <argument type="service" id="setono_sylius_trustpilot.order_eligibility_checker" />
            <argument type="service" id="setono_sylius_trustpilot.email_manager" />
            <argument type="service" id="sylius.manager.order" />
        </service>

    </services>
</container>
